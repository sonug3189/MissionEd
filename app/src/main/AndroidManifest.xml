        <activity android:name=".forgot_password"
            tools:ignore="Instantiatable" />
        <activity android:name=".profile_dialog"
            tools:ignore="Instantiatable" />
        <activity android:name=".profile_edit" />
        <activity android:name=".profile" />
        <activity android:name=".Activity1" />
        <activity android:name=".Activity2" />
        <activity android:name=".Activity3" />
        <activity android:name=".Activity4" />
        <activity android:name=".select_grade" />
        <activity android:name=".onboarding" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_chapter" />
        <activity android:name=".activity_notes"/>
        <activity android:name=".activity_quizoes"/>
        <activity android:name=".activity_test"/>
        <activity android:name=".tutor_signup"/>
        <activity android:name=".signup_option"/>
    </application>

</manifest>